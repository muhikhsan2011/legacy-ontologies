viso-graphic:Containment_Relation
      rdf:type owl:Class ;
      rdfs:label "Containment"^^xsd:string ;
      rdfs:subClassOf viso-graphic:Object-to-Object_Relation ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty viso-graphic:uses ;
                owl:someValuesFrom viso-graphic:Container
              ] ;
      dct:description <http://www-st.inf.tu-dresden.de/semvis/ontologies/rvl/img/avm_containment.png> , <http://www-st.inf.tu-dresden.de/semvis/ontologies/viso/img/vr_containment.png> , "The containment relation is actually binary, but this class is still needed to allow for properly assigning the roles container and containee to the involved graphic objects."^^xsd:string ;
      swstatus:term_status
              "stable"^^xsd:string ;
      skos:editorialNote "TODO: Having this n-ary version of modelling containment is also required, when the area contained by overlapping containers should be modelled. However, it could be argued, that this is already handled by superimposition. "^^xsd:string .

