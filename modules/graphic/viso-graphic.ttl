viso-graphic:Animated_Graphic_Object
      rdf:type owl:Class ;
      rdfs:label "Animated Graphic Object" ;
      rdfs:subClassOf viso-graphic:Graphic_Object ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf (viso-graphic:Graphic_Object [ rdf:type owl:Restriction ;
                            owl:onProperty viso-graphic:has_graphic_attribute ;
                            owl:someValuesFrom viso-graphic:Temporal_Attribute
                          ])
              ] .

viso-graphic:Animated_Graphic_Representation
      rdf:type owl:Class ;
      rdfs:label "Animated Graphic Representation" ;
      rdfs:subClassOf viso-graphic:Graphic_Representation ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf (viso-graphic:Graphic_Representation [ rdf:type owl:Restriction ;
                            owl:onProperty viso-graphic:has_graphic_attribute ;
                            owl:someValuesFrom viso-graphic:Temporal_Attribute
                          ])
              ] .

