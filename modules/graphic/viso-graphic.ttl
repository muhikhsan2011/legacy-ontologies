viso-graphic:Composite_Graphic_Object
      rdf:type owl:Class ;
      rdfs:label "Composite Graphic Object" ;
      rdfs:subClassOf viso-graphic:Graphic_Object ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty viso-graphic:consists_of ;
                owl:someValuesFrom viso-graphic:Syntactic_Structure
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty viso-graphic:consists_of ;
                owl:someValuesFrom viso-graphic:Graphic_Object
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty viso-graphic:consists_of ;
                owl:someValuesFrom viso-graphic:Graphic_Space
              ] ;
      owl:equivalentClass
              [ rdf:type owl:Class ;
                owl:intersectionOf (viso-graphic:Graphic_Object [ rdf:type owl:Restriction ;
                            owl:onProperty viso-graphic:consists_of ;
                            owl:someValuesFrom viso-graphic:Graphic_Object
                          ])
              ] ;
      swstatus:term_status
              "stable"^^xsd:string .

viso-graphic:Composite_Metric_Space
      rdf:type owl:Class ;
      rdfs:label "Composite Metric Space" ;
      rdfs:subClassOf viso-graphic:Metric_Space ;
